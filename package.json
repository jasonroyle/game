{
  "scripts": {
    "build": "npm run tsc && npm run sass",
    "build:watch": "npm run build && concurrently --raw \"tsc:watch\" \"npm run sass:watch\"",
    "destroy": "rimraf app",
    "postinstall": "rimraf typings && typings install",
    "rebuild": "npm run destroy && npm run build",
    "rebuild:watch": "npm run destroy && npm run build:watch",
    "sass": "node-sass src/theme/app.scss app/theme/app.css",
    "sass:watch": "node-sass --watch --recursive src/theme/app.scss app/theme/app.css",
    "start": "concurrently --raw \"npm run rebuild:watch\" \"lite-server\"",
    "tsc": "tsc",
    "tsc:watch": "tsc --watch"
  },
  "devDependencies": {
    "concurrently": "2.2.0",
    "lite-server": "2.2.2",
    "node-sass": "3.8.0",
    "rimraf": "2.5.3",
    "typescript": "1.8.10",
    "typings": "1.3.1"
  },
  "dependencies": {
    "core-js": "2.4.0",
    "createjs-easeljs": "0.8.2",
    "socket.io-client": "^1.4.8",
    "systemjs": "0.19.31"
  }
}
